<?xml version="1.0"?>

<page xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
      xsi:noNamespaceSchemaLocation="urn:magento:framework:View/Layout/etc/page_configuration.xsd">
    <body>
        <container name="visitor-county-container" output="1"> <!-- renders the output automatically -->
            <block class="Magento\Framework\View\Element\Template" name="evozon_cache_county" template="Evozon_Cache::county.phtml"
            cacheable="false">
                <arguments>
                    <argument name="view_model" xsi:type="object">Evozon\Cache\ViewModel\County</argument>
                </arguments>
            </block>
        </container>
    </body>
</page>
